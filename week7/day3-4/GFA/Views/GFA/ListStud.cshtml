@model List<string>
<!DOCTYPE html>
@{ 
    int count = 0;
}
<html>
<head>
    <title>List</title>
</head>
<body>
    <header> There is @Model.Count students </header>
    @foreach (var student in Model)
    {
        count++;
        <ul>
            <li>
                @student
            </li>
        </ul>
    }
</body>
</html>